{
  "env": {
    "es6": true,
    "browser": true
  },
  "rules": {
    "comma-dangle": 0,
    "indent": ["error", 2],
    "no-multiple-empty-lines": ["error", { "max": 1, "maxEOF": 0, "maxBOF": 0 }]
	},
	"parser": "babel-eslint",
	"parserOptions": {
		"sourceType": "module",
    "allowImportExportEverywhere": false
	}
}
