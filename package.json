    "type": "git",
    "url": "git+https://github.com/mariobrusarosco/api-best-shot.git"
  },
  "bugs": {
    "url": "https://github.com/mariobrusarosco/api-best-shot/issues"
  },
  "homepage": "https://github.com/mariobrusarosco/api-best-shot/#readme",
  "dependencies": {
    "@aws-sdk/client-eventbridge": "^3.716.0",
    "@aws-sdk/client-s3": "^3.703.0",
    "@aws-sdk/client-scheduler": "^3.716.0",
    "@types/lodash": "^4.17.13",
    "@types/node-cron": "^3.0.11",
    "axios": "^1.7.2",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^16.0.2",
    "drizzle-orm": "^0.32.0",
    "express": "^4.18.1",
    "file-type": "^19.6.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "mongoose": "^6.5.4",
    "node-cron": "^3.0.3",
    "postgres": "^3.4.4",
    "puppeteer": "^22.12.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.16.0",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
